{
  "name": "redux-anecdotes",
  "version": "0.1.6",
  "engines": {
    "node": "16.x"
  },
  "dependencies": {
    "axios": "0.27.2",
    "cross-env": "7.0.3",
    "json-server": "0.17.0",
    "json-server-reset": "1.3.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "8.0.2",
    "react-scripts": "5.0.1",
    "redux": "4.2.0",
    "redux-thunk": "2.4.1",
    "web-vitals": "2.1.4"
  },
  "scripts": {
    "build": "rm -rf frontend/build && react-scripts build && mv build frontend/",
    "test": "cypress run",
    "cypress:open": "cypress open",
    "eject": "react-scripts eject",
    "start": "npm run build && cross-env NODE_ENV=production node backend/server.js",
    "start:dev": "npm run build && cross-env NODE_ENV=development node backend/server.js",
    "dev": "nodemon --exec \"npm run start:dev\" -e \"js,jsx\" --ignore \"build/\" --ignore \"frontend/\" --ignore \"cypress/\" -d 2",
    "eslint": "eslint './**/*.{js,jsx}'"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cypress": "13.15.1",
    "eslint": "8.20.0",
    "nodemon": "2.0.19"
  }
}